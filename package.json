{
  "name": "emaily-app",
  "version": "1.0.0",
  "description": "Application to send emailing campaigns with Node-Express and React",
  "main": "index.js",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amelielecoz/Emaily-app.git"
  },
  "author": "amelie le coz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amelielecoz/Emaily-app/issues"
  },
  "homepage": "https://github.com/amelielecoz/Emaily-app#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.13",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^8.121.0"
  }
}
